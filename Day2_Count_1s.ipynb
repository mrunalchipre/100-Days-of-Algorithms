      "source": [
        "def count_of_bits(value):\n",
        "        n = 0\n",
        "        while value:\n",
        "                value &= value-1\n",
        "                n += 1\n",
        "        return n"
      ],
      "execution_count": 1,
      "outputs": []
    },
