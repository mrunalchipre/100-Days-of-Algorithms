      "source": [
        " def hanoi(height, left='A', right='C', middle='B'):\n",
        "        if height:\n",
        "            hanoi(height-1, left, middle, right)\n",
        "            print(left, '=>', right)\n",
        "            hanoi(height-1, middle, right, left)"
      ],
      "execution_count": null,
      "outputs": []
